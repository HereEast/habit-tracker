{
  "compilerOptions": {
    "baseUrl": ".",
    "outDir": "./dist",
    // "module": "preserve",
    // "moduleResolution": "bundler",
    "module": "ESNext",
    "moduleResolution": "bundler",
    "allowImportingTsExtensions": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "noEmit": true,
    "jsx": "react-jsx",
    "jsxImportSource": "react",
    "strict": true,
    "paths": {
      "~/*": ["src/*"],
    }
  },
  "include": ["src"]
}
